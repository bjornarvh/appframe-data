{
  "name": "@olenbetong/appframe-data",
  "version": "0.1.1",
  "description": "Classes for working with AppframeWeb data sources outside Appframe articles.",
  "main": "dist/data-object.min.js",
  "scripts": {
    "build": "npm run build:test && npm run build:prod",
    "build:test": "npx webpack --mode=development --progress --config webpack.config.js",
    "build:prod": "npx webpack --mode=production --progress --config webpack.config.js",
    "publish": "npm run publish:test && npm run publish:prod",
    "publish:test": "npx appframe publish --mode test --config appframe.config.js",
    "publish:prod": "npx appframe publish --mode production --config appframe.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/olenbetong/appframe-data.git"
  },
  "keywords": [
    "appframe",
    "appframeweb"
  ],
  "author": "Bj√∏rnar Vister Hansen <bvh@olenbetong.no>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/olenbetong/appframe-data/issues"
  },
  "homepage": "https://github.com/olenbetong/appframe-data#readme",
  "dependencies": {
    "deepmerge": "^4.0.0",
    "eventemitter3": "^4.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-typescript": "^7.3.3",
    "@olenbetong/appframe-cli": "^2.0.2",
    "@types/jest": "^26.0.23",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.0.6",
    "dotenv": "^8.1.0",
    "eslint": "^7.25.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-jest": "^24.3.6",
    "jest": "^26.6.3",
    "jest-fetch-mock": "^3.0.3",
    "minimist": "^1.2.3",
    "redux": "^4.0.4",
    "ts-jest": "^26.5.5",
    "typescript": "^4.2.4",
    "webpack": "^5.35.1",
    "webpack-cli": "^4.6.0"
  }
}
