{
  "name": "@olenbetong/appframe-data-object",
  "version": "0.1.0",
  "description": "An implementation of data objects for AppframeWeb applications.",
  "main": "appframe-data-object.js",
  "scripts": {
    "build": "npm run build:test && npm run build:prod",
    "build:test": "npx webpack --mode=development --progress --config webpack.config.js",
    "build:prod": "npx webpack --mode=production --progress --config webpack.config.js",
    "publish": "npm run publish:test && npm run publish:prod",
    "publish:test": "npx appframe publish --mode test --config appframe.config.js",
    "publish:prod": "npx appframe publish --mode production --config appframe.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bjornarvh/appframe-data-object.git"
  },
  "keywords": [
    "appframe",
    "appframeweb"
  ],
  "author": "Bj√∏rnar Vister Hansen <bvh@olenbetong.no>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bjornarvh/appframe-data-object/issues"
  },
  "homepage": "https://github.com/bjornarvh/appframe-data-object#readme",
  "dependencies": {
    "eventemitter3": "^3.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.1.6",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/preset-env": "^7.1.6",
    "@babel/preset-typescript": "^7.1.0",
    "@bjornarvh/appframe-cli": "^1.1.6",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "dotenv": "^6.1.0",
    "eslint": "^5.9.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-jest": "^22.0.0",
    "jest": "^23.6.0",
    "minimist": "^1.2.0",
    "webpack": "^4.26.0",
    "webpack-cli": "^3.1.2"
  }
}
